{
  "name": "obsidian-toolkit-plugin",
  "version": "1.0.0",
  "description": "This is an Obsidian plugin providing a collection of utility and APIs for tasks such as downloading YouTube metadata, manipulating files, and more.",
  "main": "dist/main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs --watch",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
