  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@googleapis/youtube": "^20.0.0",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "builtin-modules": "4.0.0",
    "dotenv": "^16.0.3",
    "esbuild": "0.24.2",
    "globals": "^15.14.0",
    "gts": "^6.0.2",
    "obsidian": "latest",
    "tslib": "2.8.1",
    "typescript": "^5.7.2",
    "youtubei.js": "^12.2.0"
  }
}
